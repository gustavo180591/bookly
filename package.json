		"https-browserify": "^1.0.0",
		"net-browserify": "^0.2.4",
		"os-browserify": "^0.3.0",
		"path-browserify": "^1.0.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"prisma": "^6.13.0",
		"process": "^0.11.10",
		"querystring-es3": "^0.2.1",
		"stream-browserify": "^3.0.0",
